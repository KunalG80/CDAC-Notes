<%- include('menu.ejs') %>

<div class="container">
    <h1> Data From Databse</h1>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
          </tr>
        </thead>
        <tbody>
            <% data.forEach(function(record) { %>
                <tr>
                    <td><%=record.name%></td>
                    <td><%=record.age%></td>
                </tr>
            <% }); %>
        </tbody>
      </table>
</div>